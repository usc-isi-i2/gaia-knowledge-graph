    SELECT distinct ?e ?type ?p ?cluster
    WHERE {
        ?e a aida:Relation .
        ?r rdf:subject ?e ;
           rdf:predicate rdf:type ;
           rdf:object ?type .

        ?r2 rdf:subject ?e ;
           rdf:predicate ?p ;
           rdf:object ?ent .
        GRAPH <http://www.isi.com/superedge> {
            ?mem aida:cluster ?cluster ;
                aida:clusterMember ?ent .
        }
    }
